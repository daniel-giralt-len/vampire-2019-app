{
  "players": {
      "0": {
          "id": int,
          "password": String(/^[A-J]{8}$/),
          "token": String (utid1),
          "token_timestamp": float (epoch),
          "theme": StringFromList(["light", "dark"])
      }
  },
  "narrator": {
    "type": Element,
    "password": String (unhashed),
    "token": String (utid1),
    "token_timestamp": float (epoch)
  },
  "date": {
    "type": Element,
    "id": int,
    "value": float (epoch)
  },
  "weather": {
    "type": Element,
    "id": int,
    "value": String (items from clsoed list)
  },
  "_default": {},
